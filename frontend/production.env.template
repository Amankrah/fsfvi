# FSFVI Frontend Production Configuration Template
# Copy this file to .env.production and update values as needed

# Backend API URLs
NEXT_PUBLIC_API_URL=https://fsfvi.ai
NEXT_PUBLIC_FASTAPI_URL=https://fsfvi.ai/api

# Environment
NODE_ENV=production
NEXT_PUBLIC_ENVIRONMENT=production

# Domain configuration
NEXT_PUBLIC_DOMAIN=fsfvi.ai
NEXT_PUBLIC_PROTOCOL=https

# Feature flags
NEXT_PUBLIC_ENABLE_ANALYTICS=true
NEXT_PUBLIC_ENABLE_LOGGING=true

# Security
NEXT_PUBLIC_SECURE_COOKIES=true 